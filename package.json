{
  "name": "fl-assistant",
  "title": "Assistant",
  "version": "1.5.3.1",
  "description": "A plugin to help see what's happening in WordPress and navigate to posts and pages easily.",
  "repository": {
    "type": "git",
    "url": "https://github.com/beaverbuilder/assistant.git"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "build:dist": "npm run build && node bin/build-dist.js",
    "build:bb:dist": "cross-env BB_BUILD=true npm run build:dist",
    "build:stats": "webpack --env production --json > stats.json",
    "dev": "webpack",
    "fix": "eslint src --fix",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --runInBand",
    "analyze": "cross-env NODE_ENV=analyze webpack",
    "link:packages": "node bin/link.js",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "format": "prettier -w \"src/**/*.{js,jsx,ts,tsx,css,scss,json,md}\""
  },
  "license": "GPL-2.0+",
  "dependencies": {
    "@beaverbuilder/app-core": "^0.2.11",
    "@beaverbuilder/box": "0.0.4",
    "@beaverbuilder/cloud": "^0.5.28",
    "@beaverbuilder/cloud-ui": "^0.8.67",
    "@beaverbuilder/fluid": "^0.7.23",
    "@beaverbuilder/fluid-docs": "0.0.6",
    "@beaverbuilder/forms": "^0.3.16",
    "@beaverbuilder/icons": "0.0.9",
    "@codemirror/language-data": "^6.5.1",
    "@popmotion/popcorn": "^0.4.4",
    "@uiw/react-codemirror": "4.22.2",
    "@uiw/react-textarea-code-editor": "^2.0.1",
    "array-move": "^3.0.1",
    "axios": "^0.22.0",
    "axios-cache-adapter": "^2.7.3",
    "build": "^0.1.4",
    "classnames": "^2.3.1",
    "codemirror": "^6.0.0",
    "cookie": "^0.4.1",
    "copy-to-clipboard": "^3.3.1",
    "date-fns": "^2.14.0",
    "framer-motion": "^3.10.5",
    "history": "^5.0.0",
    "html2canvas-pro": "^1.5.8",
    "md5": "^2.2.1",
    "promise": "^8.1.0",
    "qs": "^6.9.3",
    "query-string": "^6.13.8",
    "react": "^17.0.2",
    "react-clipboard.js": "^2.0.16",
    "react-color": "^2.18.1",
    "react-dom": "^17.0.2",
    "react-laag": "^2.0.3",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "use-media": "^1.4.0",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.1",
    "@babel/eslint-parser": "^7.28.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.0",
    "@babel/preset-env": "^7.13.5",
    "@babel/preset-react": "^7.12.13",
    "@beaverbuilder/eslint-config": "0.0.5",
    "@beaverbuilder/webpack-config": "0.0.9",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^13.5.0",
    "@wordpress/babel-preset-default": "^5.0.1",
    "axios-mock-adapter": "^1.18.1",
    "babel-jest": "^26.5.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "expose-loader": "^3.0.0",
    "fs-extra": "^9.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.5.0",
    "mini-css-extract-plugin": "^2.1.0",
    "npm-install-webpack-plugin": "^4.0.5",
    "postcss-loader": "^6.1.1",
    "prettier": "^3.6.2",
    "sass": "^1.69.4",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.2.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.1.4"
  }
}
